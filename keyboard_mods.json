{
    "title": "Jeff's Complex Mods",
    "description": "tmux right-alt to move left pane",
    "rules": [
        {
            "description": "Various mods for Jeff",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_gui"
                    },
                    "to": [
                        {
                            "key_code": "a",
                            "modifiers": [
                                "left_control"
                            ]
                        },
                        {
                            "key_code": "h"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.googlecode\\.iterm2$"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_alt"
                    },
                    "to": [
                        {
                            "key_code": "a",
                            "modifiers": [
                                "left_control"
                            ]
                        },
                        {
                            "key_code": "j"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.googlecode\\.iterm2$"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_control"
                    },
                    "to": [
                        {
                            "key_code": "c",
                            "modifiers": [
                                "left_control"
                            ]
                        },
                        {
                            "key_code": "c",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.googlecode\\.iterm2$"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "application"
                    },
                    "to": [
                        {
                            "key_code": "spacebar",
                            "modifiers": [
                                "left_control",
                                "left_option"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}

